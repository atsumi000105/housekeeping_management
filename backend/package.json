{
  "name": "motelmanagement",
  "version": "1.0.0",
  "description": "Motel Management Backend",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc --reporter=text --reporter=html mocha --recursive --timeout 15000",
    "test": "mocha --recursive --timeout 15000",
    "start": "nodemon web/bin/www",
    "agenda": " node worker/agenda-server/agenda",
    "debug": "set DEBUG=motel:* & nodemon web/bin/www",
    "build:api": "webpack --mode production --config webpack.api.config.js",
    "build:jobs": "webpack --mode production --config webpack.jobs.config.js",
    "start:prodAPI": "node ./APIBuild.js",
    "start:prodJobs": "node ./JobsBuild.js",
    "migrateToProd": "npx sequelize-cli db:migrate --env production",
    "seedToProd": " npx sequelize-cli db:seed:all --env production",
    "migrateToDev": " npx sequelize-cli db:migrate",
    "seedToDev": "npx sequelize-cli db:seed:all",
    "undoAllProdMigrations": "npx sequelize-cli db:seed:undo --env production | npx sequelize-cli db:migrate:undo:all --env production"
  },
  "author": "Ivan Zou",
  "license": "ISC",
  "dependencies": {
    "agenda": "^3.0.0",
    "agendash2": "^0.8.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.4",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "googleapis": "^62.0.0",
    "helmet": "^4.1.1",
    "json2csv": "^5.0.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.9.4",
    "mongoose-sequence": "^5.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.14",
    "pug": "^3.0.0",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.3.5",
    "socket.io": "^3.0.4",
    "webpack-node-externals": "^2.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "decache": "^4.6.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "proxyquire": "^2.1.3",
    "run-rs": "^0.6.2",
    "sequelize-cli": "^6.2.0",
    "sinon": "^9.2.0",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^3.5.0",
    "sinon-mongoose": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
